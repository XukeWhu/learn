package top;

import top.top124.TreeNode;

public class top437 {
	int count=0;
	public int pathSum(TreeNode root, int targetSum) {
		if(root==null)return 0;
		count=0;
		dfs(root,targetSum);
    }
	public void dfs(TreeNode root, int targetSum) {
		if(root==null)return;
		int val=root.val;
		if(root.val==targetSum)count++;
		dfs(root.left,targetSum-)
	}
}
